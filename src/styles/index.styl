body
  background: $navy

body, input, select, option, textarea
  font-family: 'Lato', sans-serif
  color: #d5dfe1

*, :after, :before
  box-sizing: border-box

ul
  padding: 0

li
  list-style: none

a
  text-decoration: none
  color: $white
